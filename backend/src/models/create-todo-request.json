{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "todoID": {
      "type": "string",
      "pattern": "^(?!\\s*$).+"
    },
    "dueDate": {
      "type": "string",
      "pattern": "^(?!\\s*$).+"
    }
  },
  "required": [
    "todoName",
    "dueDate"
  ],
  "additionalProperties": false
}